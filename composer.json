{
    "name": "pod-point/form-components",
    "description": "Commonly used form components to make it easier and more flexible to create forms in Blade views",
    "keywords": ["php", "laravel", "blade", "form", "components"],
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.2",
        "ext-dom": "*",
        "illuminate/view": "^6.20 || ^7.30 || ^8.83 || ^9.52 || ^10.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Hall",
            "email": "joe.hall@pod-point.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PodPoint\\FormComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PodPoint\\FormComponents\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
